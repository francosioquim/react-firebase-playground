{
    "files.associations": {
        "*.js": "javascriptreact"
    },
    "editor.formatOnSave": true,
    "prettier.semi": false,
    "prettier.trailingComma": "es5",
    "prettier.useTabs": false,
    "prettier.spaceBeforeFunctionParen": false,
    "prettier.arrayExpand": true,
    "prettier.arrowParens": "always",
    "prettier.tabWidth": 4,
    "prettier.flattenTernaries": false,
    "prettier.bracketSpacing": false,
    "prettier.breakProperty": false,
    "prettier.noSpaceEmptyFn": false,
    "prettier.openOutput": false,
    "prettier.bracesSpacing": false,
    "prettier.jsxBracketSameLine": false
}
